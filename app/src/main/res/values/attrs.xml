<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="title" format="string" />
    <attr name="hint" format="string" />

    <declare-styleable name="SetupFieldView">
        <attr name="title" />
        <attr name="hint" />
        <attr name="inputType" format="enum">
            <enum name="oneLine" value="0" />
            <enum name="multiLine" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SetupExpandableView">
        <attr name="title" />
        <attr name="hint" />
    </declare-styleable>

    <declare-styleable name="PlayView">
        <attr name="play_icon" format="reference" />
        <attr name="pause_icon" format="reference" />
    </declare-styleable>
</resources>